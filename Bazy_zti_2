zadanie 1
Select imie, nazwisko, d.nazwa from pracownik p inner join dzial d on p.dzial=d.id_dzialu;

zadanie 2
select t.nazwa_towaru, k.nazwa_kategori, s.ilosc from towar t inner join stan_magazynowy s on t.id_towaru=s.towar inner join kategoria k on t.id_towaru=id_kategori order by s.ilosc desc;

zadanie 3
select * from zamowienie z inner join status_zamowienia s on z.id_zamowienia=s.id_statusu_zamowienia where nazwa_statusu_zamowienia='anulowane';

zadanie 4
select * from klient k inner join adres_klienta a on k.id_klienta=a.klient where miejscowosc='Olsztyn'; 

zadanie 5


zadanie 6
select z.numer_zamowienia, nazwa_towaru, p.ilosc, p.cena from zamowienie z inner join pozycja_zamowienia p on z.id_zamowienia=p.zamowienie inner join towar t on p.towar=t.id_towaru where year(z.data_zamowienia)=2018;

zadanie 7


zadanie 98
select * from pozycja_zamowienia p inner join zamowienie z on p.zamowienie=z.id_zamowienia order by data_zamowienia asc limit 10;

zadanie 9
select * from zamowienie z inner join status_zamowienia s on z.status_zamowienia=s.id_statusu_zamowienia where nazwa_statusu_zamowienia!='zrealizowane';

zadanie 10
select * from adres_klienta where kod like '__-___';

regexp '[0-9]{2}-[0-9]{3}';
